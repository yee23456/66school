        "def count_each_char_1(string):\n",
        "    res = {}\n",
        "    for i in string:\n",
        "        if i not in res:\n",
        "            res[i] = 1\n",
        "        else:\n",
        "            res[i] += 1\n",
        "    return res\n",
        " \n",
        "print(count_each_char_1('data'))"
